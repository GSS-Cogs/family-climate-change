{
    "$schema": "http://gss-cogs.github.io/family-schemas/dataset-schema-1.1.0.json",
    "id": "beis-provisional-uk-greenhouse-gas-emissions-national-statistics-2022",
    "title": "Provisional UK greenhouse gas emissions national statistics 2022",
    "publisher": "Department for Business, Energy and Industrial Strategy",
    "description": "",
    "landingPage": "https://www.gov.uk/government/statistics/provisional-uk-greenhouse-gas-emissions-national-statistics-2022",
    "datasetNotes": [
        "On the landing page, navigate down to 2022 UK greenhouse gas emissions: provisional figures - data tables (alternative ODS)."
    ],
    "published": "2023-03-30",
    "families": [
        "Climate-Change"
    ],
    "extract": {
        "source": ""
    },
    "transform": {
        "airtable": "recVcNM2Z2Tl603q4",
        "main_issue": 748,
        "columns": {
            "Period": {
                "type": "dimension",
                "uri": "http://purl.org/linked-data/sdmx/2009/dimension#refPeriod",
                "value": "http://reference.data.gov.uk/id/{+period}"
            },
            "National Communication Sector": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/climate-change/property/dimension/national-communication-sector",
                "value": "http://gss-data.org.uk/def/climate-change/concept/national-communication-sector/{+national_communication_sector}"
            },
            "Gas": {
                "type": "dimension",
                "uri" : "http://gss-data.org.uk/def/climate-change/property/dimension/ghg",
                "value": "http://gss-data.org.uk/def/climate-change/concept/ghg/{+gas}"
            },
            "Fuel": {
                "type": "dimension",
                "new": {
                    "codelist": "./codelists/fuel.csv-metadata.json"
                }
            },
            "Area": {
                "type" : "dimension",
                "uri": "http://purl.org/linked-data/sdmx/2009/dimension#refArea",
                "value": "http://statistics.data.gov.uk/id/statistical-geography/{+area}"  
            },
            "Measure": {
                "type": "measures"
            },
            "Value": {
                "type": "observations",
                "unit": {
                    "label": "Millions of tonnes of Carbon-dioxide equivalent (MtCO2e)"
                }
            }
        }    
    },
    "sizingNotes": "",
    "notes": ""
}