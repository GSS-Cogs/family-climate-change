{
    "$schema": "http://gss-cogs.github.io/family-schemas/dataset-schema-1.1.0.json",
    "id": "defra-carbon-footprint-summary-final-demand",
    "title": "Carbon Footprint - Summary Final Demand",
    "publisher": "Department for Environment Food & Rural Affairs",
    "description": "Annual greenhouse gas and carbon dioxide emissions relating to UK consumption.",
    "landingPage": "https://www.gov.uk/government/statistics/uks-carbon-footprint",
    "datasetNotes": [
        "On the landing page, navigate down to UK full dataset 1990 - 2020, including conversion factors by SIC code (Navigate to Summary Final Demand tab)"
    ],
    "published": "2023-06-07",
    "families": [
        "Climate-Change"
    ],
    "extract": {
        "source": "ODS"
    },
    "transform": {
        "airtable": "recvyRtlVuL5wtJ0O",
        "columns": {
            "Year": {
                "type": "dimension",
                "new": {
                    "subPropertyOf": "http://purl.org/linked-data/sdmx/2009/dimension#refPeriod",
                    "codelist": true
                },
                "value": "http://reference.data.gov.uk/id/year/{+year}"
            },
            "Final Demand Breakdown": {
                "type" : "dimension",
                "new": {
                    "codelist": "./codelists/final-demand-breakdown.csv-metadata.json"
                }
            },
            "Measure": {
                "type": "measures",
                "value": "http://gss-data.org.uk/def/measure/{+measure}"
            },
            "Unit": {
                "type": "units",
                "value": "http://gss-data.org.uk/def/concept/measurement-units/{+unit}"
            },
            "Value": {
                "type": "observations",
                "datatype": "decimal"
            }
        },
        "csvcubed_uri_style": "WithoutFileExtensions"
    },
    "sizingNotes": "",
    "notes": ""
}